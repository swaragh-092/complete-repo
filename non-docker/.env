



# Application Configuration
VITE_CLIENT_KEY=non-docker
VITE_PORT=7877
VITE_APP_NAME=non-docker

# Authentication Service URLs
# Auth-service and account-ui run in Docker behind the gateway (HTTPS, no ports)
VITE_AUTH_BASE_URL=https://auth.local.test/auth
VITE_ACCOUNT_UI_URL=https://account.local.test
VITE_REDIRECT_URI=https://non-docker.local.test:7877/callback

# âœ… Organization Configuration
VITE_REQUIRES_ORGANIZATION=true
VITE_ORGANIZATION_MODEL=multi
VITE_ONBOARDING_FLOW=flexible

# Development
NODE_ENV=development

# TLS (for self-signed mkcert certificates)
NODE_TLS_REJECT_UNAUTHORIZED=0