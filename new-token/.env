




# Application Configuration
VITE_CLIENT_KEY=new-token
VITE_PORT=8769
VITE_APP_NAME=new-token

# Authentication Service URLs (HTTPS enabled)
# These are generated from SSO_CONFIG in sso-client.js
VITE_AUTH_BASE_URL=https://auth.local.test/auth
VITE_ACCOUNT_UI_URL=https://account.local.test
VITE_REDIRECT_URI=https://new-token.local.test:8769/callback

# âœ… Organization Configuration
VITE_REQUIRES_ORGANIZATION=true
VITE_ORGANIZATION_MODEL=multi
VITE_ONBOARDING_FLOW=flexible

# Development
NODE_ENV=development

# TLS (for self-signed mkcert certificates)
NODE_TLS_REJECT_UNAUTHORIZED=0